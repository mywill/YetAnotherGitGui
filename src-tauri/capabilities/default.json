{
  "$schema": "https://schema.tauri.app/config/2.0.0/capability",
  "identifier": "default",
  "description": "Default capabilities for the application",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "cli:default",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text",
    "clipboard-manager:allow-read-text",
    "updater:default",
    "process:default",
    "opener:default"
  ]
}
